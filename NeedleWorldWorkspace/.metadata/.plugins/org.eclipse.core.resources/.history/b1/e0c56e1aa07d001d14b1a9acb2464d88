package com.buxx0.needleworld.item;

import net.minecraft.world.item.ItemStack;

public class CreativeModeCombat {
	public static final CreativeModeCombat NEEDLEWORLD_COMBAT = new CreativeModeCombat("needleworld") {
		@Override
		public ItemStack makeIcon() {
			return new ItemStack(ModItems.IRON_NEEDLE.get());
		}
		
	};

}
